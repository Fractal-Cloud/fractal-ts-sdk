{
  "name": "@fractal_cloud/sdk",
  "version": "0.0.1",
  "description": "Typescript Fractal Cloud SDK",
  "license": "GPL-3.0-or-later",
  "author": "Fractal Cloud",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Fractal-Cloud/fractal-ts-sdk"
  },
  "scripts": {
    "build": "tsdown",
    "build:docs": "typedoc --entryPointStrategy Expand --exclude \"**/*+(.test|.e2e).ts\" src",
    "dev": "tsdown --watch",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "gts lint",
    "lint:fix": "gts fix",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/superagent": "^8.1.9",
    "@vitest/coverage-v8": "^4.0.18",
    "gts": "^7.0.0",
    "tsdown": "^0.20.3",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "superagent": "^10.3.0"
  }
}
